                        <div class="text-center"><button type="submit" class="btn btn-success send-message">Send
                                Message</button>
                        </div>

                    </form>
                    <div class='success-icon'>
                        <i class="far fa-check-circle fa-7x"></i>
                    </div>
                </div>
            </div>

        </div>
